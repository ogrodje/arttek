<html lang="sl">
<head>
    <meta charset="utf-8">
    <title>Ogrodje Podcast Thumbnail</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="{{server}}/css/episode-video-2">
</head>
<body class="yt podcast">
<div class="container-x yt podcast">
    <div class="row">
        <div class="col right">
            <div class="center-logo"><img src="{{server}}/raw/ogrodje-white-logo.png"/></div>
            <div class="people">
                {{#people}}
                    {{#has}}
                        <div class="box">
                            <div class="frame"><img
                                    src="https://media.graphassets.com/output=format:jpg/resize=width:800,height:800,fit:crop/{{person.picture}}"/>
                            </div>
                            <div class="fullName">{{person.person.fullName}}</div>
                        </div>
                    {{/has}}
                    {{^has}}
                        <div class="box"></div>
                    {{/has}}
                {{/people}}
            </div>
        </div>
    </div>
</div>
</body>
